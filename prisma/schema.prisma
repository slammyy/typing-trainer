generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model english_words {
  id   Int    @id @default(autoincrement())
  word String @unique
}

model records {
  id  Int @id @default(autoincrement())
  wpm Int
}

model russian_words {
  id   Int    @id @default(autoincrement())
  word String @unique
}
